			div
				select(type="text", name="tipoin", placeho der="es local ", id="tipoin", class"form-control")
					option(value="privada" selected="Privadada") Oficina Privada
					option(value="Compratido" selected="Compratido") Escritorio Compratido
					option(value="Dedicado" selected="Dedicado") Escitorio Dedicado
					option(value="Auditorio" selected="Auditorio") Auditorio
					option(value="Sala" selected="Sala") Sala de juntas
