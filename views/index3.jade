										img(src="/public/imagenes/#{imagen._id}.#{imagen.extension}")
										a(href="/detalle/#{imagen._id}" class="ordenado")
											p=imagen.dlugar
											p="$"+imagen.precio1+"/"+imagen.preriodo1
				else if imagen.tipo == filtro[2]
					if imagen.preriodo == filtro[3] || imagen.preriodo1 == filtro[3] || imagen.preriodo2 == filtro || imagen.preriodo3 == filtro[3]
						if "0a5" == filtro[4]
