										img(src="/public/imagenes/#{imagen._id}.#{imagen.extension}")
										a(href="/detalle/#{imagen._id}" class="ordenado")
											p=imagen.dlugar
											p="$"+imagen.precio1+"/"+imagen.preriodo1
						else if "25a50"
